{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": [
        "babel-plugin-macros",
        ["styled-components", { "ssr": true }],
        "relay"
      ]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": [
        "babel-plugin-macros",
        ["styled-components", { "ssr": true }],
        "relay"
      ]
    },
    "test": {
      "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]],
      "plugins": [
        "babel-plugin-macros",
        ["styled-components", { "ssr": true }],
        "relay"
      ]
    }
  }
}
