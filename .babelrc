{
  "env": {
    "development": {
      "presets": ["next/babel", "@zeit/next-typescript/babel"],
      "plugins": [
        "babel-plugin-macros",
        ["styled-components", { "ssr": true }],
        "relay"
      ]
    },
    "production": {
      "presets": ["next/babel", "@zeit/next-typescript/babel"],
      "plugins": [
        "babel-plugin-macros",
        ["styled-components", { "ssr": true }],
        "relay"
      ]
    },
    "test": {
      "presets": [
        ["next/babel", { "preset-env": { "modules": "commonjs" } }],
        "@zeit/next-typescript/babel"
      ],
      "plugins": [
        "babel-plugin-macros",
        ["styled-components", { "ssr": true }],
        "relay"
      ]
    }
  }
}
